{
    "name": "geotab/mygeotab-php",
    "type": "library",
    "description": "Unofficial PHP client for the MyGeotab API",
    "keywords": ["geotab", "mygeotab", "api", "sdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Murkovic",
            "email": "mikemurkovic@geotab.com",
            "homepage": "https://geotab.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^7.9.2",
        "monolog/monolog": "^3.8.1"
    },
    "require-dev": {
      "phpunit/phpunit": "^11.5.6"
    },
    "autoload": {
        "psr-4": {
            "Geotab\\": "src/Geotab/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geotab\\API\\Tests\\": "test/",
            "GeotabPHP\\": "examples/top-speeding-violations/src/GeotabPHP/"
        }
    }
}
